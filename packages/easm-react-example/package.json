  "repository": {
    "type": "git",
    "url": "https://github.com/dotnetautor/easm.git"
  },
  "scripts": {
    "cleanup": "shx rm -rf ./dist/*",
    "prebuild": "npm run cleanup",
    "build": "webpack --mode production --progress",
    "start": "webpack-dev-server --progress --inline --progress --env debug --content-base dist/ --host 0.0.0.0 --port 3100",
    "postbuild": "node ../../scripts/copy",
    "pack": "npm pack ./dist",
    "postpack": "mkdir -p ../../dist && mv *.tgz ../../dist",
    "docs": "typedoc -p . --theme minimal --target 'es6' --excludeNotExported --excludePrivate --ignoreCompilerErrors --exclude \"**/src/**/__tests__/*.*\" --out docs src/"
  },
  "sideEffects": false,
  "dependencies": {
    "@easm/core": "^0.2.1-beta",
    "@easm/react": "^0.2.1-beta",
    "@easm/babel-plugin-transform": "^0.2.1-beta.4",
    "@easm/ts-plugin-transform": "^0.2.1-beta.5",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "shx": "0.3.2",
    "typedoc": "0.14.2",
    "typescript": "3.3.3333",
    "typescript-tslint-plugin": "0.3.1",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "terser-webpack-plugin": "1.2.3",
    "ts-loader": "5.3.3",
    "url-loader": "1.1.2",
    "style-loader": "0.23.1",
    "postcss-loader": "3.0.0",
    "raw-loader": "1.0.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "babel-loader": "8.0.5",
    "autoprefixer": "9.4.9",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1"
  }
}